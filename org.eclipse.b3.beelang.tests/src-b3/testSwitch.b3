import java.util.List;
function main(List argv) {
	testSwitch();
}
function testSwitch() {
	
	assertEquals("simple switch", 1, switch 10 case 10: 1 endswitch); 
	assertEquals("simple switch-2", 1, switch 10 case 20: 2 case 10: 1 endswitch); 
	assertEquals("switch test against true", 1, switch case 2==20: 2 case 10==10: 1 endswitch); 
	assertEquals("switch using default/any case", 1, switch 34567 case 20: 2 case _: 1 endswitch); 
	assertEquals("switch using default/any case against true", 1, switch case 2==20: 2 case _: 1 endswitch); 
}