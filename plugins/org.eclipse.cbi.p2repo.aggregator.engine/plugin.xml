   </extension>
   <extension
         point="org.eclipse.cbi.p2repo.cli.commands">
      <command
            class="org.eclipse.cbi.p2repo.aggregator.engine.SignatureCleaner"
            name="signature-cleanup">
      </command>
   </extension>
