         point="org.eclipse.cbi.p2repo.cli.commands">
      <command
            class="org.eclipse.cbi.p2repo.aggregator.engine.Builder"
            name="aggregate">
      </command>
   </extension>
   <extension
         point="org.eclipse.cbi.p2repo.cli.commands">
      <command
            class="org.eclipse.cbi.p2repo.aggregator.engine.Modifier"
            name="modify">
